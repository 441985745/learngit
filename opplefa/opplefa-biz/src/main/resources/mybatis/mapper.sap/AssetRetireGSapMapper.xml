<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.opple.fa.sap.dao.AssetRetireGSapDAO">
	<resultMap id="generalpostingdataMap" type="generalpostingdata">
		<result property="userName"	column="USERNAME"/>		<!--  用户名                                  --> 		
		<result property="docType"	column="DOC_TYPE"/>		<!--  参考过程                               --> 		
		<result property="docDate"	column="DOC_DATE"/>		<!--  对象关键字                             --> 		
		<result property="pstngDate"	column="PSTNG_DATE"/>		<!--  源凭证的逻辑系统                       --> 		
		<result property="fisPeriod"	column="FIS_PERIOD"/>		<!--  业务事务                               --> 		
		<result property="transDate"	column="TRANS_DATE"/>		<!--  用户名 是 操作人名                     --> 		
		<result property="compCode"	column="COMP_CODE"/>			<!--   凭证抬头文本 抬头文本说明 -->     	
		<result property="assetMaino"	column="ASSETMAINO"/>		<!--  公司代码 是                            --> 		
		<result property="assetSubno"	column="ASSETSUBNO"/>		<!--  凭证中的凭证日期 是 传sap时间          --> 		
		<result property="asseTrtyp"	column="ASSETTRTYP"/>		<!--  凭证中的记帐日期 是 传sap时间          --> 		
		<result property="fId"	column="FID"/>		<!--  外键id                               --> 		

	</resultMap>

	<insert id="save" parameterType="generalpostingdata">
		insert into TB_FA_SAP_ASSETRETIRE_G
		(
		USERNAME,
		DOC_TYPE,
		DOC_DATE,
		PSTNG_DATE,
		FIS_PERIOD,
		TRANS_DATE,
		COMP_CODE,
		ASSETMAINO,
		ASSETSUBNO,
		ASSETTRTYP,
		FID
		
		)
		values
		(
		#{generalpostingdata.userName },
		#{generalpostingdata.docType },
		#{generalpostingdata.docDate },
		#{generalpostingdata.pstngDate },
		#{generalpostingdata.fisPeriod },
		#{generalpostingdata.transDate },
		#{generalpostingdata.compCode },
		#{generalpostingdata.assetMaino },
		#{generalpostingdata.assetSubno },
		#{generalpostingdata.assetTrtyp },
		#{generalpostingdata.fId }

		)

	</insert>
</mapper>

