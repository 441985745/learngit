<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.opple.fa.sap.dao.PayHeaderDocumentSapDAO">
	<resultMap id="payHeaderDocumentMap" type="payHeaderDocument">
		<result property="id"	column="ID"/>		<!--  主键                                   --> 		
		<result property="objType"	column="OBJ_TYPE"/>		<!--  参考过程                               --> 		
		<result property="objKey"	column="OBJ_KEY"/>		<!--  对象关键字                             --> 		
		<result property="objSys"	column="OBJ_SYS"/>		<!--  源凭证的逻辑系统                       --> 		
		<result property="busAct"	column="BUS_ACT"/>		<!--  业务事务                               --> 		
		<result property="username"	column="USERNAME"/>		<!--  用户名 是 操作人名                     --> 		
		<result property="headerTxt"	column="HEADER_TXT"/>			<!--   凭证抬头文本 抬头文本说明 -->     	
		<result property="compCode"	column="COMP_CODE"/>		<!--  公司代码 是                            --> 		
		<result property="docDate"	column="DOC_DATE"/>		<!--  凭证中的凭证日期 是 传sap时间          --> 		
		<result property="pstngDate"	column="PSTNG_DATE"/>		<!--  凭证中的记帐日期 是 传sap时间          --> 		
		<result property="transDate"	column="TRANS_DATE"/>		<!--  换算日期                               --> 		
		<result property="fiscYear"	column="FISC_YEAR"/>		<!--  会计年度                               --> 		
		<result property="fisPeriod"	column="FIS_PERIOD"/>		<!--  会计期间                               --> 		
		<result property="docType"	column="DOC_TYPE"/>		<!--  凭证类型 是 kz                         --> 		
		<result property="refDocNo"	column="REF_DOC_NO"/>		<!--  参考凭证编号 是 付款单据编号           --> 		
		<result property="acDocNo"	column="AC_DOC_NO"/>		<!--  会计凭证号码 sap回传的                 --> 		
		<result property="objKeyR"	column="OBJ_KEY_R"/>		<!--  取消: 对象码 (awref_rev and aworg_rev) --> 		
		<result property="reasonRev"	column="REASON_REV"/>		<!--  冲销原因                               --> 		
		<result property="compoAcc"	column="COMPO_ACC"/>		<!--  acc 界面中的组件                       --> 		
		<result property="refDocNoLong"	column="REF_DOC_NO_LONG"/>		<!--  参考凭证号(对相关性参看长文本)         --> 		
		<result property="accPrinciple"	column="ACC_PRINCIPLE"/>		<!--  会计原则                               --> 		
		<result property="negPostng"	column="NEG_POSTNG"/>		<!--  标识: 反记帐                           --> 		
                       	

	</resultMap>

	<insert id="save" parameterType="payHeaderDocument">
	<selectKey resultType="java.lang.Long" order="AFTER" keyProperty="ID"> 
			SELECT SEQ_FA_SAP_PREPAY.CURRAL AS IID FROM DUAL 
	</selectKey>
		insert into TB_FA_SAP_PREPAY
		(
		<!-- DOC_TYPE, -->
		ID,
		OBJ_TYPE,
		OBJ_KEY,
		OBJ_SYS,
		BUS_ACT,
		USERNAME,
		HEADER_TXT,
		COMP_CODE,
		DOC_DATE,
		PSTNG_DATE,
		TRANS_DATE,
		FISC_YEAR,
		FIS_PERIOD,
		DOC_TYPE,
		REF_DOC_NO,
		AC_DOC_NO,
		OBJ_KEY_R,
		REASON_REV,
		COMPO_ACC,
		REF_DOC_NO_LONG,
		ACC_PRINCIPLE,
		NEG_POSTNG

		)
		values
		(
		SEQ_FA_SAP_PREPAY.NEXTVAL(),
		#{payHeaderDocument.objType },
		#{payHeaderDocument.objKey },
		#{payHeaderDocument.objSys },
		#{payHeaderDocument.busAct },
		#{payHeaderDocument.username },
		#{payHeaderDocument.headerTxt },
		#{payHeaderDocument.compCode },
		#{payHeaderDocument.docDate },
		#{payHeaderDocument.pstngDate },
		#{payHeaderDocument.transDate },
		#{payHeaderDocument.fiscYear },
		#{payHeaderDocument.fisPeriod },
		#{payHeaderDocument.docType },
		#{payHeaderDocument.refDocNo },
		#{payHeaderDocument.acDocNo },
		#{payHeaderDocument.objKeyR },
		#{payHeaderDocument.reasonRev },
		#{payHeaderDocument.compoAcc },
		#{payHeaderDocument.refDocNoLong },
		#{payHeaderDocument.accPrinciple },
		#{payHeaderDocument.negPostng }

		)

	</insert>
</mapper>

