package com.opple.fa.assetcard.entity;

public enum APPROVASTATUS {
	// 待审批
	WAITING("0"),
	// 审批中
	PROCESSING("1"),
	// 退回
	DENY("2"),
	// 已完成
	DONE("3");
	private String approvaStatus;

	APPROVASTATUS(String approvaStatus) {
		this.approvaStatus = approvaStatus;
	}

	public String getApprovaStatus() {
		return approvaStatus;
	}
}
