<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean id="purchaseCommonDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.purchase.dao.PurchaseCommonDAO" />
	</bean>
     <!-- 收货单 -->
	<!-- DAO -->
	<bean id="purchaseOrderDetailDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.purchase.dao.PurchaseOrderDetailDAO" />
	</bean>
	<bean id="receiveGoodsDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.purchase.dao.ReceiveGoodsDAO" />
	</bean>
	<bean id="receiveGoodsDetailDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.purchase.dao.ReceiveGoodsDetailDAO" />
	</bean>
	<bean id="receiveGoodsMappingDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.purchase.dao.ReceiveGoodsMappingDAO" />
	</bean>
	<!-- Service -->
	<bean id="purchaseCommonService" class="com.opple.fa.purchase.service.impl.PurchaseCommonServiceImpl">
		<property name="purchaseCommonDAO" ref="purchaseCommonDAO"/>
	</bean>
	<bean id="purchaseOrderDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.purchase.dao.PurchaseOrderDAO" />
	</bean>
	<bean id="administrationAssetDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.assetcard.dao.AdministrationAssetDAO" />
	</bean>
	<bean id="workFlowMapper" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.common.dao.WorkFlowMapper" />
	</bean>

	<bean id="receiveGoodsMappingService" class="com.opple.fa.purchase.service.impl.ReceiveGoodsMappingServiceImpl">
<!-- 		<constructor-arg ref="transactionManager"/> -->
		<property name="administrationAssetDAO" ref="administrationAssetDAO"/>
		<property name="purchaseOrderDAO" ref="purchaseOrderDAO"/>
		<property name="purchaseOrderDetailDAO" ref="purchaseOrderDetailDAO"/>
		<property name="applyOrderDAO" ref="applyOrderDAO"/>
		<property name="applyOrderDetailDAO" ref="applyOrderDetailDAO"/>
		<property name="receiveGoodsDAO" ref="receiveGoodsDAO"/>
		<property name="receiveGoodsDetailDAO" ref="receiveGoodsDetailDAO"/>
		<property name="purchaseCommonDAO" ref="purchaseCommonDAO"/>
		<property name="receiveGoodsMappingDAO" ref="receiveGoodsMappingDAO"/>
		<property name="computerAssetDAO" ref="computerAssetDAO"/>
		<property name="generalAssetDAO" ref="generalAssetDAO"/>
		<property name="machineryEquipmentDAO" ref="machineryEquipmentDAO"/>
		<property name="measuringInstrumentsDAO" ref="measuringInstrumentsDAO"/>
		<property name="workFlowMapper" ref="workFlowMapper"/>
	</bean>
	
	<bean id="receiveGoodsService" class="com.opple.fa.purchase.service.impl.ReceiveGoodsServiceImpl">
		<property name="receiveGoodsDAO" ref="receiveGoodsDAO"/>
		<property name="receiveGoodsDetailDAO" ref="receiveGoodsDetailDAO"/>
		<property name="receiveGoodsMappingDAO" ref="receiveGoodsMappingDAO"/>
		<property name="generalAssetDAO" ref="generalAssetDAO"/>
		<property name="administrationAssetDAO" ref="administrationAssetDAO"/>
		<property name="computerAssetDAO" ref="computerAssetDAO"/>
		<property name="machineryEquipmentDAO" ref="machineryEquipmentDAO"/>
		<property name="measuringInstrumentsDAO" ref="measuringInstrumentsDAO"/>
		<property name="purchaseCommonDAO" ref="purchaseCommonDAO"/>
		<property name="checkAcceptMappingDAO" ref="checkAcceptMappingDAO"/>
		<property name="applyOrderDetailDAO" ref="applyOrderDetailDAO"/>
	</bean>
	<bean id="receiveGoodsDetailService" class="com.opple.fa.purchase.service.impl.ReceiveGoodsDetailServiceImpl">
		<property name="receiveGoodsDetailDAO" ref="receiveGoodsDetailDAO"/>
	</bean>
	
	
	<!-- 验收单 -->
	<!-- DAO -->
	<bean id="applyOrderDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.purchase.dao.ApplyOrderDAO" />
	</bean>
	<bean id="applyOrderDetailDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.purchase.dao.ApplyOrderDetailDAO" />
	</bean>
	<bean id="checkAcceptanceDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.purchase.dao.CheckAcceptanceDAO" />
	</bean>
	<bean id="checkAcceptanceDetailDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.purchase.dao.CheckAcceptanceDetailDAO" />
	</bean>
	<bean id="checkAcceptMappingDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.purchase.dao.CheckAcceptMappingDAO" />
	</bean>
	<!-- 资产卡片的 -->
	<bean id="computerAssetDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.assetcard.dao.ComputerAssetDAO" />
	</bean>
		<bean id="generalAssetDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.assetcard.dao.GeneralAssetDAO" />
	</bean> 
	<bean id="machineryEquipmentDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.assetcard.dao.MachineryEquipmentDAO" />
	</bean>
	<bean id="measuringInstrumentsDAO" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.assetcard.dao.MeasuringInstrumentsDAO" />
	</bean>


	<!-- sap李凯接口 -->
 	<bean id="sapUtilService" class="org.mybatis.spring.mapper.MapperFactoryBean" parent="baseDAO">
		<property name="mapperInterface" value="com.opple.fa.sap.service.impl.SapUtilServiceImpl" />
	</bean>
	<!-- Service -->
	<bean id="checkAcceptMappingService" class="com.opple.fa.purchase.service.impl.CheckAcceptMappingServiceImpl">
		<property name="checkAcceptanceDAO" ref="checkAcceptanceDAO"/>
		<property name="checkAcceptanceDetailDAO" ref="checkAcceptanceDetailDAO"/>
		<property name="checkAcceptMappingDAO" ref="checkAcceptMappingDAO"/>
		<property name="purchaseCommonDAO" ref="purchaseCommonDAO"/>
		<property name="computerAssetDAO" ref="computerAssetDAO"/>
		<property name="generalAssetDAO" ref="generalAssetDAO"/>
		<property name="machineryEquipmentDAO" ref="machineryEquipmentDAO"/>
		<property name="measuringInstrumentsDAO" ref="measuringInstrumentsDAO"/>
		<property name="sapUtilService" ref="sapUtilService"/>
		<property name="administrationAssetDAO" ref="administrationAssetDAO"/>
		<property name="purchaseOrderDAO" ref="purchaseOrderDAO"/>
		<property name="demandOrderDetailDAO" ref="demandOrderDetailDAO"/>
		<property name="purchaseOrderDetailDAO" ref="purchaseOrderDetailDAO"/>

	</bean>
	<bean id="checkAcceptanceService" class="com.opple.fa.purchase.service.impl.CheckAcceptanceServiceImpl">
		<property name="checkAcceptanceDAO" ref="checkAcceptanceDAO"/>
	</bean>
	<bean id="checkAcceptanceDetailService" class="com.opple.fa.purchase.service.impl.CheckAcceptanceDetailServiceImpl">
		<property name="checkAcceptanceDetailDAO" ref="checkAcceptanceDetailDAO"/>

	</bean>
</beans>