	<!-- 查询sap资产明细 -->
	<select id="searchPurchaseCommonByReceiveGoodsId" resultMap="purchaseCommonResultMap">
	   SELECT *
	     FROM TB_FA_PURCHASE_COMMON PC
	    WHERE 1 = 1
