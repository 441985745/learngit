<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.opple.fa.sap.dao.Extension2SapDAO">
	<resultMap id="extension2Map" type="extension2">
		<result property="prId"	column="PR_ID"/>		
		<result property="structure"	column="STRUCTURE"/>	<!--bapi 表扩展的结构名称--> 
		<result property="valuepart1"	column="VALUEPART1"/>	<!--bapi 扩展参数的数据部分--> 
		<result property="valuepart2"	column="VALUEPART2"/>	<!--bapi 扩展参数的数据部分--> 
		<result property="valuepart3"	column="VALUEPART3"/>	<!--bapi 扩展参数的数据部分--> 
		<result property="valuepart4"	column="VALUEPART4"/>	<!--bapi 扩展参数的数据部分--> 

                       	

	</resultMap>

	<insert id="save" parameterType="extension2">
		insert into TB_FA_SAP_EXTENSION2
		(
		PR_ID,
		STRUCTURE,
		VALUEPART1,
		VALUEPART2,
		VALUEPART3,
		VALUEPART4

		)
		values
		(
		#{prePayDoc.prId },
		#{prePayDoc.structure },
		#{prePayDoc.valuepart1 },
		#{prePayDoc.valuepart2 },
		#{prePayDoc.valuepart3 },
		#{prePayDoc.valuepart4 }

		)

	</insert>
</mapper>

