#downmsg_emessage {
    POSITION: fixed;
    _position: absolute;
    z-index: 100;
    bottom: 0;
    right: 0;
    background: url(../Images/down_msg_bg.gif) no-repeat left top;
    width: 225px;
}

#donwmsg_head {
    float: left;
    display: inline;
    font-size: 12px;
    color: #FF0000;
    margin-left: 26px;
    margin-top: 6px;
    font-weight: bold;
}

#downmsgBar .close {
    float: right;
    width: 11px;
    height: 11px;
    margin-top: 6px;
    display: block;
    margin-right: 5px;
}

#downmsgBar {
    height: 25px;
}

#donwmsg_content {
    height: 162px;
    overflow: scroll;
    background: url(../Images/yhbg.gif);
}

#donwmsg_content ul {
    font-size: 12px;
    color: #007cc1;
    top: 0px;
    padding: 0px 2px 0 6px;
    left: 6px;
    line-height: 180%;
    height: 162px;
    margin-top: 5px;
}

#donwmsg_content ul li {
    background: url(../Images/down_msg_bg.gif) no-repeat -100px -245px;
    text-indent: 13px;
}

#donwmsg_content ul li a {
    color: #007cc1;
}

#donwmsg_content ul .ll a {
    color: #663399;
    font-weight: bold;
}

#donwmsg_content ul .KK a {
    color: #ff6347;
    font-weight: bold;
}

a.msg-hidden-btn-1 {
    width: 11px;
    height: 11px;
    overflow: hidden;
    float: right;
    display: block;
    margin-right: 5px;
    margin-top: 6px;
    background: url(../Images/down_msg_bg.gif) no-repeat -50px -250px;
}

a.msg-hidden-btn-2 {
    width: 11px;
    height: 11px;
    overflow: hidden;
    float: right;
    display: block;
    margin-right: 5px;
    margin-top: 6px;
    background: url(../Images/down_msg_bg.gif) no-repeat -0px -250px;
}
